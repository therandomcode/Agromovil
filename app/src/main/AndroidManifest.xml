<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wagner.agromovil">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FarmerHomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FarmerBeginRequestPickup" />
        <activity android:name=".FarmerEditProfile" />
        <activity android:name=".FarmerViewHistory" />
        <activity android:name=".FarmerViewHistoryDetail" />
        <activity android:name=".FarmerRequestPickupPickDate" />
        <activity android:name=".FarmerRequestPickupSetPickupLocation" />
        <activity android:name=".FarmerRequestPickupSetPickupLocationType" />
        <activity android:name=".FarmerRequestPickupSetDropoffLocation" />
        <activity android:name=".FarmerRequestPickupChooseTransporter" />
        <activity android:name=".FarmerRequestPickupViewTransporterDetails" />
        <activity android:name=".FarmerRequestPickupEnterPayment" />
        <activity android:name=".FarmerRequestPickupReviewOrder" />
        <activity android:name=".FarmerRequestPickupOrderConfirmation"></activity>
    </application>

</manifest>